        <item quantity="one">Detská Slina</item>
        <item quantity="other">Detské Sliny</item>
    </plurals>
    <string name="inventory_item_baby_drool_description">Prečo by sa na Zemi mali udržovať detské sliny v zásobe FPS?!?
    </string>
    <plurals name="inventory_item_king_crown_title">
        <item quantity="one">Kráľovská Koruna</item>
        <item quantity="other">Kráľovské Koruny</item>
    </plurals>
    <string name="inventory_item_king_crown_description">Krásne a žiadané šperky. Ak sa rozhodnete ich roztaviť získate obrovské množstvo zlata.
    </string>
    <plurals name="inventory_item_steel_bullet_title">
        <item quantity="one">Oceľová Guľka</item>
        <item quantity="other">Oceľové Guľky</item>
    </plurals>
    <string name="inventory_item_steel_bullet_description">Keď držíte oceľovú guľku v ruke, môžete cítiť jej silu. Používanie týchto guliek Vám udelí +1 bonus poškodenia.
    </string>
    <plurals name="inventory_item_gold_bullet_title">
        <item quantity="one">Zlatá Guľka</item>
        <item quantity="other">Zlaté Guľky</item>
    </plurals>
    <string name="inventory_item_gold_bullet_description">Guľky vyrobené zo zlata sú pre duchov velice vražedné.
        Používanie týchto guliek Vám udelí +3 bonus poškodenia.
    </string>
    <plurals name="inventory_item_one_shot_bullet_title">
        <item quantity="one">\"Jednorázová\" Guľka</item>
        <item quantity="other">\"Jednorázové\" Guľky</item>
    </plurals>
    <string name="inventory_item_one_shot_bullet_description">
        Guľka, ktorej sila je tak veľká, že môže zabiť viacerých duchov naraz. Tento typ guliek je veľmi drahý, tak s nimi zaobchádzajte opatrne. Používanie týchto guliek Vám udelí +10 bonus poškodenia.
    </string>
    <plurals name="inventory_item_ghost_tear_title">
        <item quantity="one">Slza ducha</item>
        <item quantity="other">Slzy ducha</item>
    </plurals>
    <string name="inventory_item_ghost_tear_description">
        Malá slza pochádza z matky ducha, ktorá sa snažila zachrániť jej duchov. Táto malá kvapka Vám môže priniesť mimoriadnu silu.
    </string>
    <plurals name="inventory_item_speed_potion_title">
        <item quantity="one">Rýchlosť elixíru</item>
        <item quantity="other">Rýchlosť elixírov</item>
    </plurals>
