        <item quantity="one">Kúsok zlámanej helmy</item>
        <item quantity="other">Kúsky zlámaných heliem</item>
    </plurals>
    <string name="inventory_item_broken_helmet_horn_description">Malý kúsok ocele pochádza z prilby ducha. Môže byť užitočný pri výrobe niečoho z ocele.
    </string>
